    "pg": "^8.13.3",
    "@hono/zod-validator": "^0.4.2",
    "zod": "^3.24.1",
    "jose": "^5.9.6",
    "nanoid": "^5.1.2"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.11",
    "tsx": "^4.19.2",
    "drizzle-kit": "^0.30.4"
